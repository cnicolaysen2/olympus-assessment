{
  "name": "slb-olympus",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve --host 0.0.0.0 --port 4200",
    "start:alter": "ng serve --host 0.0.0.0 --port 3000",
    "build": "ng build --configuration=dev && workbox injectManifest",
    "build:prod": "ng build --prod --configuration=prod",
    "deploy": "npm run build && cd dist && firebase deploy && cd -",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "predeploy:gcloud": "npm run build:prod",
    "deploy:gcloud": "gcloud app deploy --version=prod -q",
    "postdeploy:gcloud": "gcloud app browse",
    "deploy:browse": "gcloud app browse"
  },
